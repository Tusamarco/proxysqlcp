<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"> <xsl:import href="dataset.xsl"/> <xsl:output omit-xml-declaration="yes" method="xml" encoding="UTF-8" indent="yes" />  <xsl:template match="/dataset_prew" >  	  <xsl:variable name="datasetid" select="dataset/@id" />	  <xsl:variable name="type" select="dataset/@type" />	  <xsl:variable name="datasetstyle" select="dataset/@style" />	  <xsl:variable name="datasetname" select="dataset/@name" />	        <xsl:variable name="countryname" select="dataset/xml/datasets/country/@name" />	<table style='{$datasetstyle}'>		<tr>			<td>				Dataset id			</td>			<td>				Dataset type			</td>			<td>				Country Name			</td>		</tr>		<tr>			<td>				<xsl:value-of select="$datasetid"/>			</td>			<td>				<xsl:value-of select="$type"/>			</td>			<td>				<xsl:value-of select="$countryname"/>			</td>		</tr>		<tr>			<td colspan="3">				<xsl:call-template name="dataset"></xsl:call-template>						</td>		</tr>	</table>    </xsl:template></xsl:stylesheet>