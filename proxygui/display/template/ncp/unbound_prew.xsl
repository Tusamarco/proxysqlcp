<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"> <xsl:import href="unbound.xsl"/><xsl:output omit-xml-declaration="yes" method="xml" encoding="UTF-8" indent="yes" />  <xsl:template match="unbound_prew" >  	  <xsl:variable name="textid" select="unbound/@id" />	  <xsl:variable name="type" select="unbound/@type" />	  <xsl:variable name="textstyle" select="unbound/@style" />	  <xsl:variable name="textname" select="unbound/@name" />	<table style='{$textstyle}'>		<tr>			<td>				text  id			</td>			<td>				text type			</td>		</tr>		<tr>			<td>				<xsl:value-of select="$textid"/>			</td>			<td>				<xsl:value-of select="$type"/>			</td>		</tr>		<tr>			<td colspan="2">				<xsl:call-template name="unbound"></xsl:call-template>			</td>		</tr>	</table>  </xsl:template></xsl:stylesheet>